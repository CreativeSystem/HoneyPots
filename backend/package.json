{
  "name": "@honey-pots/backend",
  "version": "1.0.0",
  "license": "MIT",
  "main": "server.ts",
  "scripts": {
    "lint": "eslint --fix src",
    "prestart": "docker-compose up -d",
    "start": "tsnd -r tsconfig-paths/register --ignore node_modules --transpile-only --no-notify --respawn --inspect -- src/server.ts",
    "pretest": "NODE_ENV=test yarn typeorm migration:run",
    "test": "NODE_ENV=test jest --passWithNoTests",
    "posttest": "NODE_ENV=test yarn typeorm schema:drop",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm --config ./src/config/typeorm.ts"
  },
  "config": {
    "commitizen": {
      "path": "./backend/node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "bcrypt": "^5.0.0",
    "cors": "^2.8.5",
    "date-fns": "^2.16.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fb": "^2.0.0",
    "helmet": "^4.1.0",
    "inversify": "^5.0.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "ms": "^2.1.2",
    "pg": "^8.3.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.25",
    "typeorm-naming-strategies": "^1.1.0",
    "uuid": "^8.3.1",
    "validatorjs": "^3.19.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@commitlint/core": "^11.0.0",
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.7",
    "@types/date-fns": "^2.6.0",
    "@types/express": "^4.17.7",
    "@types/facebook-js-sdk": "^3.3.1",
    "@types/factory-girl": "^5.0.2",
    "@types/faker": "^4.1.12",
    "@types/fb": "^0.0.28",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^26.0.10",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.1",
    "@types/ms": "^0.7.31",
    "@types/node": "^14.6.0",
    "@types/uuid": "^8.3.0",
    "@types/validatorjs": "^3.15.0",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "commitizen": "^4.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-import-resolver-typescript": "^2.2.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-standard": "^4.0.1",
    "factory-girl": "^5.0.4",
    "faker": "^5.1.0",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.1",
    "sort-package-json": "^1.44.0",
    "sqlite": "^4.0.14",
